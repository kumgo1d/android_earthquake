<?xml version="1.0" encoding="utf-8"?>
<!-- EarthquakeSearchProvider에 필요한 메타데이터 정의 -->
<!--content:// 스키마가 사용된 단순 URI 주소 -->
<searchable
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:label="@string/app_name"
